CREATE DATABASE poe_data;

CREATE TABLE tabs (
uid integer PRIMARY KEY,
id integer,
n varchar[40],
owner integer REFERENCES accounts(id),
b integer,
r integer,
g integer
);

CREATE TABLE accounts (
 id integer PRIMARY KEY,
 name varchar[40],
 sessid varchar[32]
 );
 
 CREATE TABLE characters (
  id integer PRIMARY KEY,
  name varchar[40],
  accountid integer REFERENCES accounts(id)
  );
 
 
 CREATE TABLE acqproperties(
  id integer PRIMARY KEY,
  name varchar[40],
  value1 real,
  value2 real
  )
  
  
  
  
  CREATE TABLE items (
   id integer PRIMARY KEY,
   gggid varchar[64],
   name varchar[40],
   ilvl integer
   owner integer REFERENCES characters(id),
   propeties integer REFERENCES acqproperties(id),
   x integer,
   y integer,
   z integer
   );
   
 
 